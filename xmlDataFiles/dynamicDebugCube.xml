<DynamicDebugCube>
   
    <Properties>
        <Scale x='5' y='5' z='5' />
        <Rotation x='90' y='90' z='90' />
        <Translation x='50' y='0' z='50' random='true' />
    </Properties>
    

    <GOC_Effect>
        <Emittor>
            <Properties>
                <Offset x='0.0f' y='0.0f' z='0.0f' />
                <Duration time = '-1.0f' />
                <ParticleTexture texture='data/textures/Red.png'/>
                <ParticleBaseLifespan time='5.0f'/>
                <ParticleScaleStart x='1.0f' y='1.0f' z='1.0f' />
                <ParticleScaleEnd x='7.0f' y='7.0f' z='7.0f' />  
            </Properties>

            <ShapeBox>
                <Dimensions width='4.0f' height='4.0f' length='4.0f' />     
            </ShapeBox>
            
            <EmissionModeContinuous>
                <EmissionDelay time='0.005f' />
            </EmissionModeContinuous>

            <AffectorLinearMove>
                <Direction x="0.0f" y= "7.0f" z="0.0f" />
                <IsMoveOnParticleDirection isMOPD="true" />
            </AffectorLinearMove>

            <AffectorScale>
            </AffectorScale>      
        </Emittor>
    </GOC_Effect>


    <GOC_Effect>
        <Emittor>
            <Properties>
                <Offset x='0.0f' y='0.0f' z='0.0f' />
                <Duration time = '-1.0f' />
                <ParticleTexture texture='data/textures/Mosaic.png' />
                <ParticleBaseLifespan time='5.0f'/>
                <ParticleScaleStart x='1.0f' y='1.0f' z='1.0f' />
                <ParticleScaleEnd x='7.0f' y='7.0f' z='7.0f' />  
            </Properties>

            <ShapePoint>     
            </ShapePoint>
            
            <EmissionModeContinuous>
                <EmissionDelay time='0.05f' />
            </EmissionModeContinuous>

            <AffectorLinearMove>
                <Direction x="0.0f" y= "16.0f" z="0.0f" />
                <IsMoveOnParticleDirection isMOPD="false" />
            </AffectorLinearMove>

            <AffectorScale>
            </AffectorScale>
        </Emittor>
    </GOC_Effect>
   
    <GOC_BoundingSphere />
    
</DynamicDebugCube>